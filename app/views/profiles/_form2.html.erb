<div class="animated fadeIn">
  <div class="form">
  <%= form_for @profile do |f| %>
    <%= f.label :dob %>
    <%= f.date_field :dob %>

    <%= f.label :bio %>
    <%= f.text_field :bio %>

    <%= f.label :location %>
    <%= f.text_field :location %>

    <%= f.label :website %>
    <%= f.text_field :website %>
<!--
    <%= f.label :musical_dreamteam %>
    <%= f.text_field :musical_dreamteam %> -->

    <%= f.label :avatar %>
    <%= f.file_field :avatar %>


    <%= f.submit %>
  <%end%>
 </div>
 </div>
